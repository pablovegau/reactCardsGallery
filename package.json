{
  "name": "reactcardsgallery",
  "version": "1.0.0",
  "description": "Card gallery created with React",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"**/*.{js,jsx}\"",
    "format": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.{js,jsx,css,json}\"",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "pretest:e2e:run": "npm run prod",
    "test:e2e:run": "start-server-and-test start http://localhost:8080/ cy:run",
    "test:e2e:dev": "start-server-and-test start http://localhost:8080/ cy:open",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "npm run lint && npm run prettier -- --list-different && npm test && npm run test:coverage && npm run test:e2e:run",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "dev": "npm run webpack -- --env.mode development",
    "prod": "npm run webpack -- --env.mode production",
    "prod:analyze": "npm run prod -- --env.presets analyze",
    "prod:compress": "npm run prod -- --env.presets compress",
    "start": "npm run webpack-dev-server -- --env.mode development --watch --open --hot",
    "phoenix": "rm -rf node_modules dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test && npm run test:coverage && npm run test:e2e:run",
      "pre-push": "lint-staged && npm test && npm run test:coverage && npm run test:e2e:run"
    }
  },
  "author": "Pablo Vega Uceta",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "cypress": "^3.1.5",
    "cypress-testing-library": "^2.3.6",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jest-axe": "^3.1.1",
    "jest-dom": "^3.1.3",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.8.4",
    "react-testing-library": "^6.0.0",
    "start-server-and-test": "^1.7.11",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.3",
    "uniqid": "^5.0.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1"
  }
}
