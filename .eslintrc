{
    "extends": ["eslint:recommended", "prettier"],
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "globals": {
        "React": true
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off"
    }
}
