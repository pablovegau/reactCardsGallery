{
    "extends": [
        "eslint:recommended",
        "prettier",
        "prettier/react",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended"
    ],
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jest": true,
        "cypress/globals": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["react", "prettier", "jsx-a11y", "cypress"],
    "globals": {
        "React": true
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off"
    }
}
